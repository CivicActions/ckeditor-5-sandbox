{
  "name": "cke_5_sandbox_cms_ds_components",
  "version": "1.0.0",
  "description": "Bundled CMS Design System React components for use in Drupal",
  "targets": {
    "help-drawer": {
      "context": "browser",
      "source": "./src/help-drawer/help-drawer.html",
